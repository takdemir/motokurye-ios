<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tbmyazilim.www" version="1.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Kurye Otomasyon Sistemleri</name>
    <description>
        Kurye Otomasyon Sistemleri
    </description>
    <author email="taneryzb@hotmail.com" href="https://tbmsoft.xyz">
        https://tbmsoft.xyz
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.3" />
    <plugin name="cordova-plugin-console" source="npm" spec="~1.0.3" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.0" />
    <plugin name="cordova-plugin-vibration" spec="^2.1.5" />
    <plugin name="cordova-plugin-camera" spec="^2.4.1" />
    <plugin name="cordova-plugin-insomnia" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.3" />
    <platform name="android">
        <icon density="mdpi" src="www/res/icon/android/mipmap-mdpi/Courier-24848.png" />
        <icon density="hdpi" src="www/res/icon/android/mipmap-hdpi/Courier-27272.png" />
        <icon density="xhdpi" src="www/res/icon/android/mipmap-xhdpi/Courier-29696.png" />
        <icon density="xxhdpi" src="www/res/icon/android/mipmap-xxhdpi/Courier-2144144.png" />
        <icon density="xxxhdpi" src="www/res/icon/android/mipmap-xxxhdpi/Courier-2192192.png" />
    </platform>
    <platform name="ios">
    </platform>
    <platform name="wp8">
        <icon height="99" platform="wp8" src="www/res/icon/wp8/ApplicationIcon.png" width="99" />
        <icon height="159" platform="wp8" src="www/res/icon/wp8/Background.png" width="159" />
        <splash height="1280" platform="wp8" src="www/res/screen/wp8/screen-portrait.jpg" width="768" />
    </platform>
    <platform name="windows">
    </platform>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-android-play-services-gradle-release" spec="~1.1.3" />
    <plugin name="org.flybuy.cordova.background-location-services" spec="https://github.com/pmwisdom/cordova-background-geolocation-services.git" />
    <engine name="ios" spec="~4.4.0" />
    <plugin name="phonegap-plugin-push" spec="~1.8.0">
        <variable name="SENDER_ID" value="809436805306" />
    </plugin>
    <plugin name="cordova-plugin-file" spec="~5.0.0" />
    <plugin name="at.gofg.sportscomputer.powermanagement" spec="https://github.com/Viras-/cordova-plugin-powermanagement.git" />
</widget>
